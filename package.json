    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "sequelize-cli": "^6.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
